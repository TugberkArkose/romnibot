<launch>
  <!-- https://github.com/ros-planning/navigation/issues/206 -->
  <include file="$(find romni_bringup)/launch/3dsensor.launch">
    <arg name="rgb_processing" value="false" />
    <arg name="depth_registration" value="false" />
    <arg name="depth_processing" value="false" />

    <arg name="scan_topic" value="/scan" />

  <!--- Run gmapping -->
  <include file="$(find romni_bringup)/launch/gmapping.launch" />

  <!--- Run Move Base -->
  <include file="$(find romni_bringup)/launch/move_base.launch" />

</launch>
